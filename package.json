{
  "name": "ember-config-service",
  "version": "1.0.0",
  "description": "Access config values from config/environment.js with a simple service",
  "keywords": [
    "ember-addon",
    "config",
    "configuration",
    "environment",
    "service"
  ],
  "repository": "https://github.com/gmurphey/ember-config-service",
  "license": "MIT",
  "author": "Garrett Murphey <gmurphey@gmurphey.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "postpublish": "git push origin $(git branch | grep '*' | sed 's/* //'); git push origin --tags"
  },
  "dependencies": {
    "ember-cli-babel": "^7.18.0",
    "ember-getowner-polyfill": "2.2.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.15.2",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-htmlbars-inline-precompile": "^3.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.2",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^7.10.1",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.9.2"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
